{
  "manifest_version": 2,

  "name": "quickScrape",
  "description": "A simple way to extract web contents off any webpage.",
  "version": "1.0.0",
   "background": { 
    "scripts": [
      "background.js"
  ]},
  "browser_action": {
    "default_icon": "logo_small.png"
  },
    "icons": {
    "128" : "logo_small.png"
  	},
	"permissions": [
	    "tabs",
	    "contextMenus",
	    "http://*/*",
	    "https://*/*"
	]
}